  "__stdlib__": {
    "scaffold": {
      "$schema": "math/base@v1.0",
      "base_alias": "gamma",
      "alias": "gamma",
      "pkg_desc": "evaluate the gamma function",
      "desc": "evaluates the gamma function",
      "short_desc": "gamma function",
      "parameters": [
        {
          "name": "x",
          "desc": "input value",
          "type": {
            "javascript": "number",
            "jsdoc": "number",
            "c": "double",
            "dtype": "float64"
          },
          "domain": [
            {
              "min": "-infinity",
              "max": "infinity"
            }
          ],
          "rand": {
            "prng": "random/base/uniform",
            "parameters": [
              -10,
              10
            ]
          },
          "example_values": [
            1,
            3.5,
            4.5,
            -0.5,
            2,
            3,
            -3.5,
            0.1,
            4,
            1.5,
            5,
            0.5,
            2.5,
            -1.5,
            -2.5,
            50,
            100,
            -50.5,
            -100.5,
            2.2
          ]
        }
      ],
      "output_policy": "real_floating_point_and_generic",
      "returns": {
        "desc": "function value",
        "type": {
          "javascript": "number",
          "jsdoc": "number",
          "c": "double",
          "dtype": "float64"
        }
      },
      "keywords": [
        "gamma",
        "factorial"
      ],
      "extra_keywords": []
    }

  "__stdlib__": {
    "scaffold": {
      "$schema": "math/base@v1.0",
      "base_alias": "digamma",
      "alias": "digamma",
      "pkg_desc": "evaluate the digamma function for a double-precision floating-point number",
      "desc": "evaluates the digamma function for a double-precision floating-point number",
      "short_desc": "digamma",
      "parameters": [
        {
          "name": "x",
          "desc": "input value",
          "type": {
            "javascript": "number",
            "jsdoc": "number",
            "c": "double",
            "dtype": "float64"
          },
          "domain": [
            {
              "min": "-infinity",
              "max": "infinity"
            }
          ],
          "rand": {
            "prng": "random/base/uniform",
            "parameters": [
              0.5,
              20
            ]
          },
          "example_values": [
            0.51,
            1.37,
            2.93,
            4.18,
            7.64,
            -2.47,
            -1.23,
            -3.79,
            -0.61,
            5.42,
            9.08,
            12.57,
            15.33,
            -4.62,
            -6.14,
            18.91,
            10.26,
            -5.37,
            3.41,
            0.89
          ]
        }
      ],
      "output_policy": "real_floating_point_and_generic",
      "returns": {
        "desc": "function value",
        "type": {
          "javascript": "number",
          "jsdoc": "number",
          "c": "double",
          "dtype": "float64"
        }
      },
      "keywords": [
        "derivative",
        "gamma",
        "digamma",
        "scalar",
        "number",
        "psi"
      ],
      "extra_keywords": []
    }
  }
}
